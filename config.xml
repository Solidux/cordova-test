<?xml version='1.0' encoding='utf-8'?>
<widget id="sv.com.chuckle.app.ios.Itest" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://phonegap.com/ns/1.0">
    <name>ITest</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="rurouni.nestor@gmail.com" href="http://chuckles.xyz">
        Chuckles Developer Team
    </author>
	<plugin name="cordova-plugin-inappbrowser" spec="1.0.1" source="pgb" />
	<plugin name="cordova-plugin-device" spec="1.0.1" source="pgb" />
	<icon src="icon.png" />
	<preference name="permissions" value="none"/>
	<feature name="http://api.phonegap.com/1.0/device"/>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
		
